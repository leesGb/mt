	<s:Button id="sameWidthBtn" x="10" y="84" width="26" height="20" label="" toolTip="匹配宽度.选中的组件统一最大的宽值"/>
	<s:Button id="sameHeightBtn" x="38" y="84" width="26" height="20" label="" toolTip="匹配高.选中的组件统一最大的高"/>
	<s:Button id="sameWHBtn" x="67" y="84" width="26" height="20" label="" toolTip="匹配宽高.选中的组件统一最大的宽、高"/>
