	<s:Button id="disBottomBtn" x="57" y="-79" width="26" height="20" label="" toolTip="底部分布"/>
	<s:Button id="disLeftBtn" x="105" y="-79" width="26" height="20" label="" toolTip="左侧分布"/>
	<s:Button id="disXCenterBtn" x="133" y="-79" width="26" height="20" label="" toolTip="水平居中分布"/>
