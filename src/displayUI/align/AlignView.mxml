	<s:Button id="alignXRightBtn" x="67" y="36" width="26" height="20" label="" toolTip="右对齐"/>
	<s:Button id="alignTopBtn" x="115" y="36" width="26" height="20" label="" toolTip="顶对齐"/>
	<s:Button id="alignYCenterBtn" x="143" y="36" width="26" height="20" label="" toolTip="垂直居中对齐"/>
	<s:Button id="alignBottomBtn" x="172" y="36" width="26" height="20" label="" toolTip="底对齐"/>
